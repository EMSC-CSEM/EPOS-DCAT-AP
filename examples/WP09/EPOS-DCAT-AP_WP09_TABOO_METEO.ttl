	<webservices.ingv.it/ingvws/nfo_taboo/meteo/operation> a hydra:Operation;
		hydra:method "GET"^^xsd:string;
		hydra:returns "json";
		hydra:IriTemplate[ a hydra:IriTemplate;
			hydra:template "http://webservices.ingv.it/ingvws/nfo_taboo/meteo/1/query(?param_name, mintime, maxtime, minlat, maxlat, minlon, maxlon, inst_type)"^^xsd:string;
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "param_name"^^xsd:string;
					rdfs:label "Meteorological parameter at CO2 site";
					http:paramValue "water_level";
					http:paramValue "air_temperature_ext";
					http:paramValue "air_temperature_int";
					http:paramValue "water_temperature";
					http:paramValue "water_conductivity";
					http:paramValue "rain";
					http:paramValue "humidity";
					http:paramValue "atmospheric_pressure";
					schema:defaultValue "humidity";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "mintime"^^xsd:string;
					rdfs:label "Minimum time";
					schema:minValue "2015-06-01T00:00:00.000Z";
					schema:maxValue "2017-12-31T00:00:00.000Z";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxtime"^^xsd:string;
					rdfs:label "Maximum time";
					schema:minValue "2015-06-01T00:00:00.000Z";
					schema:maxValue "2017-12-31T00:00:00.000Z";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minlat"^^xsd:string;
					rdfs:label "Minimum Latitude [deg]";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "43.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxlat"^^xsd:string;
					rdfs:label "Maximum Latitude [deg]";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "44.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minlon"^^xsd:string;
					rdfs:label "Minimum Longitude [deg]";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "12.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxlon"^^xsd:string;
					rdfs:label "Maximum Longitude [deg]";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "13.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "inst_type"^^xsd:string;
					rdfs:label "nickname of type of installation";
					http:paramValue "indoor";
					http:paramValue "shelter";
					http:paramValue "borehole";
					http:paramValue "soil";
					schema:defaultValue "indoor";
					hydra:required "true"^^xsd:boolean;
				];
		] ;
. 
