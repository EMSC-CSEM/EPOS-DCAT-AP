<#Seismology> a skos:ConceptScheme;
		dct:title "Seismology";
		dct:description "It contains the concepts of the Seismology domain"; 
		.

<#SeismicWaveform> a skos:Concept;
	dct:description "Measurement of the dynamic displacement of the Earth";
	skos:inScheme <#Seismology>;
	skos:prefLabel "Seismic waveform" ;
.

#-------------------------------------SHAKEMAP------------------------------
<orfeus-eu.org/odcws/rrsm/1/shakemap> a epos:WebService;
				schema:identifier "orfeus-eu.org/odcws/rrsm/1/shakemap";
				schema:identifier [ a schema:PropertyValue;
	          schema:propertyID  "DDSS-ID";
	          schema:value   "WP8-DDSS-048";
	      ];
        schema:description "Resources can be used as input files for USGS ShakeMap program to create event shakemaps.";
				schema:name "ShakeMap Input XML - Rapid Raw Strong Motion (RRSM)";
				dcat:contactPoint <http://orcid.org/0000-0002-6250-2474/contactPoint> ;
				dct:issued "2017-04-01T00:00:00Z"^^xsd:dateTime;
				dct:modified "2017-04-01T00:00:00Z"^^xsd:dateTime;
				schema:provider <PIC:999518944>;
				dcat:theme  <#SeismicWaveform> ;
 				schema:keywords "Seismic Hazard", "ODC", "RRSM", "Event", "ShakeMap", "ShakeMapXML", "Strong Motion";
				hydra:supportedOperation <ORFEUS/ODCWS/RRSM/shakemap>;
.

<ORFEUS/ODCWS/RRSM/shakemap> a hydra:Operation;
 			hydra:method "GET"^^xsd:string;
			hydra:returns "xml";
 			hydra:IriTemplate[ a hydra:IriTemplate;
            hydra:template "http://www.orfeus-eu.org/odcws/rrsm/1/shakemap/(?id,type)"^^xsd:string;
            hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "id"^^xsd:string;
               hydra:required "true"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
            	hydra:variable "type"^^xsd:string;
            	hydra:required "true"^^xsd:boolean;

            ];
          ];
.

#--------------- DATASELECT
<orfeus-eu.org/fdsnws/dataselect/1/> a epos:WebService;
 				schema:identifier "orfeus-eu.org/fdsnws/dataselect/1/";
				schema:identifier [ a schema:PropertyValue;
	          schema:propertyID  "DDSS-ID";
	          schema:value   "WP8-DDSS-001";
	      ];
        schema:description "FDSN Standard webservice at ODC to download waveform data";
				schema:name "FDSN Dataselect - ORFEUS Data Center (ODC)";
        dcat:contactPoint <http://orcid.org/0000-0002-6250-2474/contactPoint>;
				dct:issued "2016-01-01T00:00:00Z"^^xsd:dateTime;
				dct:modified "2016-01-01T00:00:00Z"^^xsd:dateTime;
				schema:provider <PIC:999518944>;
				hydra:entrypoint "http://www.orfeus-eu.org/fdsnws/dataselect/1/application.wadl"^^xsd:anyURI;
				dcat:theme  <#SeismicWaveform> ;
				schema:keywords "seismology", "seismicity", "earthquakes", "waveform", "seismic hazard", "earth structure", "earthquake intensity", "macroseismic", "macroseismic information", "waveform modeling", "ODC", "Dataselect", "FDSN-WS", "Seismic Waveform", "EIDA";
				hydra:supportedOperation <orfeus-eu.org/fdsnws/dataselect/1/query>;
    .


<orfeus-eu.org/fdsnws/dataselect/1/query> a hydra:Operation;
 				hydra:method "GET"^^xsd:string;
				hydra:returns "binary";
 				hydra:IriTemplate[ a hydra:IriTemplate;
              hydra:template "http://www.orfeus-eu.org/fdsnws/dataselect/1/query/(?starttime,endtime,network,station,location,channel,minimumlength,nodata)"^^xsd:string;
              hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "starttime"^^xsd:dateTime;
               hydra:required "true"^^xsd:boolean;
              ];
              hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "endtime"^^xsd:dateTime;
               hydra:required "true"^^xsd:boolean;
              ];
              hydra:mapping[ a hydra:IriTemplateMapping;
               	hydra:variable "network"^^xsd:string;
               	hydra:required "false"^^xsd:boolean;
               ];
               hydra:mapping[ a hydra:IriTemplateMapping;
               	hydra:variable "station"^^xsd:string;
               	hydra:required "false"^^xsd:boolean;
               ];
                hydra:mapping[ a hydra:IriTemplateMapping;
               	hydra:variable "location"^^xsd:string;
               	hydra:required "false"^^xsd:boolean;
               ];
               hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "channel"^^xsd:string;
                hydra:required "false"^^xsd:boolean;
              ];
            #  hydra:mapping[ a hydra:IriTemplateMapping;
            #   hydra:variable "quality"^^xsd:string;
            #   hydra:required "false"^^xsd:boolean;
            # ];
               hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "minimumlength"^^xsd:int;
               hydra:required "false"^^xsd:boolean;
               schema:minValue "0";
           ];

           hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "nodata"^^xsd:int;
               hydra:required "false"^^xsd:boolean;
               #schema:minValue "0";
           ];

       ];
.

#------------------------STATION

<orfeus-eu.org/fdsnws/station/1/> a epos:WebService;
        schema:identifier "orfeus-eu.org/fdsnws/station/1/";
				schema:identifier [ a schema:PropertyValue;
	          schema:propertyID  "DDSS-ID";
	          schema:value   "WP8-DDSS-049";
	      ];
        schema:description "FDSN Standard webservice at ODC to download seismic metadata";
				schema:name "FDSN Station-WS - ORFEUS Data Center (ODC)";
        dcat:contactPoint <http://orcid.org/0000-0002-6250-2474/contactPoint>;
				dct:issued "2016-01-01T00:00:00Z"^^xsd:dateTime;
				dct:modified "2016-01-01T00:00:00Z"^^xsd:dateTime;
				schema:provider <PIC:999518944>;
        dcat:theme  <#SeismicWaveform> ;
				hydra:entrypoint "http://webservices.ingv.it/fdsnws/station/1/application.wadl"^^xsd:anyURI;
        schema:keywords "seismology", "seismicity", "earthquakes", "waveform", "seismic hazard", "earth structure", "earthquake intensity", "macroseismic", "macroseismic information", "waveform modeling" ;
        hydra:supportedOperation <orfeus-eu.org/fdsnws/station/1/query>;
.

<orfeus-eu.org/fdsnws/station/1/query> a hydra:Operation;
 hydra:method "GET"^^xsd:string;
 hydra:returns "xml";
 hydra:IriTemplate[ a hydra:IriTemplate;
              hydra:template "http://www.orfeus-eu.org/fdsnws/station/1/query/(?starttime,endtime,network,station,location,channel,minlatitude,maxlatitude,minlongitude,maxlongitude,level,nodata)"^^xsd:string;
              hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "starttime"^^xsd:dateTime;
               hydra:required "true"^^xsd:boolean;
              ];
              hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "endtime"^^xsd:dateTime;
               hydra:required "true"^^xsd:boolean;
              ];
              hydra:mapping[ a hydra:IriTemplateMapping;
               	hydra:variable "network"^^xsd:string;
               	hydra:required "false"^^xsd:boolean;
                schema:defaultValue "_NFOTABOO";
               ];
               hydra:mapping[ a hydra:IriTemplateMapping;
               	hydra:variable "station"^^xsd:string;
               	hydra:required "false"^^xsd:boolean;
               ];
                hydra:mapping[ a hydra:IriTemplateMapping;
               	hydra:variable "location"^^xsd:string;
               	hydra:required "false"^^xsd:boolean;
               ];
               hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "channel"^^xsd:string;
                hydra:required "false"^^xsd:boolean;
              ];
              hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "minlatitude"^^xsd:float;
                hydra:required "false"^^xsd:boolean;
                schema:minValue "-90";
                schema:maxValue "90";
              ];
               hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "maxlatitude"^^xsd:float;
               hydra:required "false"^^xsd:boolean;
               schema:minValue "-90";
               schema:maxValue "90";
           ];
           hydra:mapping[ a hydra:IriTemplateMapping;
             hydra:variable "minlongitude"^^xsd:float;
             hydra:required "false"^^xsd:boolean;
             schema:minValue "-180";
             schema:maxValue "180";
           ];
           hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "maxlongitude"^^xsd:float;
               hydra:required "false"^^xsd:boolean;
               schema:minValue "-180";
               schema:maxValue "180";
           ];
           hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "level"^^xsd:string;
               hydra:required "false"^^xsd:boolean;
               #schema:minValue "network";
               #schema:minValue "station";
               #schema:minValue "channel";
               #schema:minValue "response";
           ];
           hydra:mapping[ a hydra:IriTemplateMapping;
               hydra:variable "nodata"^^xsd:string;
               hydra:required "false"^^xsd:boolean;
               #schema:minValue "0";
           ];

       ];
.
