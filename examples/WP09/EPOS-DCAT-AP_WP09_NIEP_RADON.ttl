    <radon.infp.ro/operation> a hydra:Operation;
        hydra:method "GET"^^xsd:string;
        hydra:returns "application/json";
        hydra:property[ a hydra:IriTemplate;
            hydra:template "https://radon.infp.ro/query(?mintime, maxtime, minlat, maxlat, minlon, maxlon, max_radon_error)"^^xsd:string;
                hydra:mapping[ a hydra:IriTemplateMapping;
                    hydra:variable "mintime"^^xsd:string;
                    rdfs:range "datetime";
                    rdfs:label "minimum time";
                    schema:minValue "2000-01-01T00:00:00.0000Z";
                    schema:maxValue "2018-09-20T12:00:00.0000Z";
                    schema:defaultValue "2000-03-01T00:00:00.0000Z";
                    hydra:required "true"^^xsd:boolean;
                ];
                hydra:mapping[ a hydra:IriTemplateMapping;
                    hydra:variable "maxtime"^^xsd:string;
                    rdfs:range "datetime";
                    rdfs:label "maximum time";
                    schema:minValue "2000-01-01T00:00:00.0000Z";
                    schema:maxValue "2018-09-20T12:00:00.0000Z";
                    schema:defaultValue "2001-05-01T00:00:00.0000Z";
                    hydra:required "true"^^xsd:boolean;
                ];
                hydra:mapping[ a hydra:IriTemplateMapping;
                    hydra:variable "minlat"^^xsd:string;
                    rdfs:range "float";
                    rdfs:label "minimum Latitude";
                    schema:minValue "-90";
                    schema:maxValue "90";
                    schema:defaultValue "-90";
                    hydra:required "true"^^xsd:boolean;
                ];
                hydra:mapping[ a hydra:IriTemplateMapping;
                    hydra:variable "maxlat"^^xsd:string;
                    rdfs:range "float";
                    rdfs:label "maximum Latitude";
                    schema:minValue "-90";
                    schema:maxValue "90";
                    schema:defaultValue "90";
                    hydra:required "true"^^xsd:boolean;
                ];
                hydra:mapping[ a hydra:IriTemplateMapping;
                    hydra:variable "minlon"^^xsd:string;
                    rdfs:range "float";
                    rdfs:label "minimum Longitude";
                    schema:minValue "-180";
                    schema:maxValue "180";
                    schema:defaultValue "-180";
                    hydra:required "true"^^xsd:boolean;
                ];
                hydra:mapping[ a hydra:IriTemplateMapping;
                    hydra:variable "maxlon"^^xsd:string;
                    rdfs:range "float";
                    rdfs:label "maximum Longitude";
                    schema:minValue "-180";
                    schema:maxValue "180";
                    schema:defaultValue "180";
                    hydra:required "true"^^xsd:boolean;
                ];
                hydra:mapping[ a hydra:IriTemplateMapping;
                    hydra:variable "max_radon_error"^^xsd:string;
                    rdfs:range "int";
                    rdfs:label "maximum error";
                    schema:minValue "0";
                    schema:maxValue "100";
                    schema:defaultValue "100";
                    hydra:required "true"^^xsd:boolean;
                ];
        ] ;
. 



