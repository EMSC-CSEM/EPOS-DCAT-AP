		<dct:spatial> <!-- Geographic location/spatial extent /bounding box -->
			<dct:Location>
				<!-- The collection covers the entire world -->
				<locn:geometry>POLYGON((-180 -90,-180 90,180 90,180 -90,-180 -90))</locn:geometry>
			</dct:Location>
		</dct:spatial>
		<adms:representationTechnique>raster</adms:representationTechnique>
		<dct:temporal> <!-- The collection covers all the satellite measurements -->
			<dct:PeriodOfTime>
				<schema:startDate>2001-12-31T12:00:00</schema:startDate>
				<schema:endDate>2124-12-31T12:00:00</schema:endDate>
			</dct:PeriodOfTime>
		</dct:temporal>
