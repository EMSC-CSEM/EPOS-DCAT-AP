	<http://space.fmi.fi/cgi-bin/imagecgi/image-epos.cgi/operation> a hydra:Operation;
		hydra:method "GET"^^xsd:string;
		hydra:returns "txt";
		hydra:property[ a hydra:IriTemplate;
			hydra:template "http://space.fmi.fi/cgi-bin/imagecgi/image-epos.cgi(?start, yourname, institute, email, submit)"^^xsd:string;
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "start"^^xsd:integer;
					rdfs:label "Date (yyyymmdd)";
					schema:minValue "19821001";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "yourname"^^xsd:string;
					rdfs:label "Name";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "institute"^^xsd:string;
					rdfs:label "Institute";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "email"^^xsd:string;
					rdfs:label "Email";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "submit"^^xsd:string;
					rdfs:label "Submit";
					http:paramValue "OK";
					hydra:required "true"^^xsd:boolean;
				];
		] ;
.
