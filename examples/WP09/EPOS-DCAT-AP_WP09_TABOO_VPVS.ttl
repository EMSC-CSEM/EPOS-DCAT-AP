	<webservices.ingv.it/ingvws/nfo_taboo/vpvs/operation> a hydra:Operation;
		hydra:method "GET"^^xsd:string;
		hydra:returns "json";
		hydra:IriTemplate[ a hydra:IriTemplate;
			hydra:template "http://webservices.ingv.it/ingvws/nfo_taboo/vpvs/1/query(?mintime, maxtime, minlat, maxlat, minlon, maxlon, mineqdepth, maxeqdepth, minnp, minns, maxpw, maxsw, minps, maxgap, midi, maxherr, maxverr, nome finto)"^^xsd:string;
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "mintime"^^xsd:string;
					rdfs:label "Minimum Time";
					schema:minValue "2001-12-31T12:00:00.000Z";
					schema:maxValue "2017-06-20T12:00:00.000Z";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxtime"^^xsd:string;
					rdfs:label "Maximum Time";
					schema:minValue "2001-12-31T12:00:00.000Z";
					schema:maxValue "2017-06-20T12:00:00.000Z";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minlat"^^xsd:string;
					rdfs:label "Minimum Latitude";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "43.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxlat"^^xsd:string;
					rdfs:label "Maximum Latitude";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "44.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minlon"^^xsd:string;
					rdfs:label "Minimum Longitude";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "12.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxlon"^^xsd:string;
					rdfs:label "Maximum Longitude";
					schema:minValue "-90";
					schema:maxValue "90";
					schema:defaultValue "13.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "mineqdepth"^^xsd:string;
					rdfs:label "Minimum Earthquake Depth";
					schema:minValue "-1";
					schema:maxValue "100";
					schema:defaultValue "0.4";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxeqdepth"^^xsd:string;
					rdfs:label "Maximum Earhquake Depth";
					schema:minValue "-1";
					schema:maxValue "100";
					schema:defaultValue "5.0";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minnp"^^xsd:string;
					rdfs:label "Number of p-waves onset observations";
					schema:defaultValue "2";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minns"^^xsd:string;
					rdfs:label "Number of s-waves onset observations";
					schema:defaultValue "3";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxpw"^^xsd:string;
					rdfs:label "P-phase onset Hypoinverse standard quality weight";
					schema:maxValue "4";
					schema:defaultValue "4";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxsw"^^xsd:string;
					rdfs:label "S-phase onset Hypoinverse standard quality weight";
					schema:maxValue "4";
					schema:defaultValue "4";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minps"^^xsd:string;
					rdfs:label "Number of P and S couples per earthquake";
					schema:defaultValue "4";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxgap"^^xsd:string;
					rdfs:label "Degrees of azimuthal stations distribution gap per earthquake";
					schema:maxValue "360";
					schema:defaultValue "150";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "midi"^^xsd:string;
					rdfs:label "Horizontal distance of the closest station from a earthquake";
					schema:minValue "0.00";
					schema:maxValue "40000.00";
					schema:defaultValue "70";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxherr"^^xsd:string;
					rdfs:label "Maximum allowed formal horizontal error in earthquake location";
					schema:minValue "0.00";
					schema:maxValue "40000.00";
					schema:defaultValue "300";
					hydra:required "true"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxverr"^^xsd:string;
					rdfs:label "Maximum allowed formal vertical error in earthquake location";
					schema:minValue "0.00";
					schema:maxValue "6378.00";
					schema:defaultValue "200";
					hydra:required "true"^^xsd:boolean;
				];
		] ;
.

