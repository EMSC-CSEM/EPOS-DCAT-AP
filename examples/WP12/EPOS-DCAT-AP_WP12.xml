	<!-- EPOS WP12 WebServices -->

	<eposap:WebService>
		<dct:title>Wrapped Differential Interferograms (Phase and Amplitude)</dct:title> <!-- title -->
		<!-- abstract -->
		<dct:description>Maps of surface deformation using differences in the phase of the waves returning to the satellite over the measurement time span. Wrapped interferograms measure only deformation relative to the satellite wavelenght (fringes).</dct:description>
		<dct:issued>2001-12-31T12:00:00</dct:issued><!-- date of publication -->
		<dct:modified>2016-12-31T12:00:00</dct:modified><!-- date of revision -->
		<dct:license>https://creativecommons.org/licenses/by-sa/3.0/</dct:license> <!-- Access and Use Restrictions -->
		<foaf:page> <!-- URI -->
			<foaf:primaryTopic>https://catalog.terradue.com/gep-epos/search?format=atomeop&amp;pt=WRAPPED_INTERFEROGRAMS</foaf:primaryTopic>
		</foaf:page>
		<dct:format>
			<dct:MediaTypeOrExtent>geoTiff</dct:MediaTypeOrExtent>
		</dct:format>
		<dct:rights>
			<dct:RightsStatement>open data</dct:RightsStatement>
		</dct:rights>
		<dct:conformsTo>EPSG:4326</dct:conformsTo> <!-- Spatial Reference System -->
		<dct:identifier>epos-gep-WRAPPED_INTERFEROGRAMS</dct:identifier> <!-- Unique Resource Identifier: this is the entry point of the service -->
		<dct:created>2001-12-31T12:00:00</dct:created> <!-- date of creation -->
		<eposap:domain>Satellite data</eposap:domain>
		<eposap:subDomain>InSAR</eposap:subDomain>
		<dcat:keyword>insar, interferogram, phase</dcat:keyword> <!-- Keywords -->
		<eposap:operation>download, visualise, query</eposap:operation>
		<dct:hasVersion>1.1</dct:hasVersion>

		<!-- List of Parameters:
			this section is needed in order to describe the parameters of the service.
			E.g. in the case of a RESTful service, we may have "domain" as parameter,
			with type "string" and a list of allowed value terms specified by the value tag,
			e.g. 'seism','gps'  (i.e. namespace)-->
		<eposap:parameter>
			<http:paramName>start</http:paramName><!-- this is the start time of the measurements -->
			<rdf:label>time:start</rdf:label>
			<dct:type>date</dct:type><!-- date format is UTC ISO yyyy-mm-ddThh:mm:ssZ -->
		</eposap:parameter>
		<eposap:parameter>
			<http:paramName>stop</http:paramName><!-- this is the end time of the measurements -->
			<rdf:label>time:end</rdf:label>
			<dct:type>date</dct:type><!-- date format is UTC ISO yyyy-mm-ddThh:mm:ssZ -->
		</eposap:parameter>
		<eposap:parameter>
			<http:paramName>bbox</http:paramName><!-- filter result over a given area of interest as a bounding box-->
			<rdf:label>geo:box</rdf:label>
			<dct:type>bbox</dct:type><!-- bbox format is a comma separated string, with corners of the rectangular bounding box: latN,lonE,latS,lonW -->
		</eposap:parameter>
		<eposap:parameter>
			<http:paramName>geom</http:paramName><!-- filter result over a given area of interest as a generic geometry -->
			<rdf:label>geo:geometry</rdf:label>
			<dct:type>geometry</dct:type><!-- geometry format is a WKT -->
		</eposap:parameter>
		<eposap:parameter>
			<http:paramName>track</http:paramName><!-- filter result for a given orbit track -->
			<rdf:label>eop:track</rdf:label>
			<dct:type>integer</dct:type>
		</eposap:parameter>
		<schema:documentation>URL pointing to the web documentation</schema:documentation><!-- documentation for the products -->
		<dcat:contactPoint>CNRFrancescoCasu</dcat:contactPoint><!-- internal link to contact point (vcard:Individual) described in the same file in <eposap:Person> -->
		<!-- internal link to responsibleParty (vcard:Organisation) described in the same file in <eposap:Organisation>.
             in the WP12 there is more than one publisher per each service, but only the main responsible organization for
             the service is included below. In any case, the web service results will contain the information of the publisher
			 for each product returned. 			 -->
